/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
        Nand(a=b,b=sel,out=out1);
        Not(in=sel,out=nsel);
        Nand(a=a,b=nsel,out=out2);
        Nand(a=out1,b=out2,out=out);
  
    
}
